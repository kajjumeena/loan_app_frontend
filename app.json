    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "apiUrl": "https://loan-app-backend-v98y.onrender.com/api",
      "eas": {
        "projectId": "dbb947ec-142f-4aef-a5c2-b46c8da67794"
